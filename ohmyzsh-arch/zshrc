# If not running interactively, do not do anything
[[ $- != *i* ]] && return
[[ $TERM != screen* ]] && test -z "$TMUX" && (tmux attach || tmux new-session) && exit

# Path to your oh-my-zsh configuration.
export DOTFILES=$HOME/.dotfiles

# Let the magic begin
source $DOTFILES/ohmyzsh-common/zshrc
for config_file ($DOTFILES/ohmyzsh-arch/*.zsh) source $config_file

# my aliases
alias clip='xclip -selection c'
alias ksr='source $HOME/kde/bashrc; cs'
x=$(cat /proc/cpuinfo|grep -c processor)
