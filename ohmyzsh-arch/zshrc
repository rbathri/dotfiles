# Path to your oh-my-zsh configuration.
export DOTFILES=$HOME/.dotfiles
export COMMON_DOTFILES=$DOTFILES/ohmyzsh-common
export PLATFORM_DOTFILES=$DOTFILES/ohmyzsh-arch

# Let the magic begin
source $COMMON_DOTFILES/zshrc
#above will also source $PLATFORM_DOTFILES ;)

# my aliases
alias clip='xclip -selection c'
alias ksr='source $HOME/kde/bashrc; cs'
x=$(cat /proc/cpuinfo|grep -c processor)
